      "target": "api::technology.technology"
    },
    "url_project": {
      "type": "string"
    },
    "github_project": {
      "type": "string"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "content": {
      "type": "richtext"
    }
  }
